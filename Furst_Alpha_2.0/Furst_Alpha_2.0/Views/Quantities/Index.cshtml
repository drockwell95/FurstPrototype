@model IEnumerable<Furst_Alpha_2._0.Models.Quantities>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.total)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.InUse)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.total)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.InUse)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.QuantityId }) |
            @Html.ActionLink("Details", "Details", new { id=item.QuantityId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.QuantityId })
        </td>
    </tr>
}

</table>
